{
  "workbench.settings.editor": "json",
  "workbench.editor.enablePreview": true,
  // EDITOR
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "blink",
  "files.trimTrailingWhitespace": true,
  "editor.fontLigatures": true,
  "editor.lineHeight": 25,
  "editor.letterSpacing": 0.5,
  "editor.cursorWidth": 5,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.fontFamily": "Operator Mono Medium,JetBrains Mono, Fira Code",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.selectionClipboard": true,
  "editor.renderIndentGuides": true,
  "editor.minimap.enabled": false,
  "editor.fontSize": 19,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.suggestSelection": "first",
  "eslint.validate": ["javascript"],
  "explorer.openEditors.visible": 0,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "[Cobalt2]": {
      "statusBar.background": "#0059ffa4",
      "tab.activeBackground": "#077bf73e",
      "gitDecoration.ignoredResourceForeground": "#706e6e"
    }
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimFinalNewlines": true,
  "files.associations": {
    "Dockerfile_*": "dockerfile"
  },
  "workbench.iconTheme": "file-icons",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "deno.enable": false,
  "window.zoomLevel": 0,
  "breadcrumbs.enabled": false,
  "workbench.colorTheme": "Nord",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "cSpell.enabled": true,
  "cSpell.language": "en",
  "cSpell.enableFiletypes": ["vue"],
  "cSpell.userWords": ["gqlpagenation", "mixins", "nestjs", "typeorm"],
  "python.linting.pylintArgs": ["--enable=E,W"],
  "python.formatting.provider": "autopep8",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateFormat": null,
  "todo-tree.tree.showScanModeButton": false
}
